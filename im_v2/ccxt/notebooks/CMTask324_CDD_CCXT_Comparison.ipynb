   "cell_type": "code",
   "execution_count": 16,
   "id": "8db077be",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2022-01-14T10:08:06.188084Z",
     "start_time": "2022-01-14T10:07:06.865713Z"
    },
    "scrolled": true
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
