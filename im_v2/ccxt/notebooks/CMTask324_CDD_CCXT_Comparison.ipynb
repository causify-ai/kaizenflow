   "source": [
    "# 'CDD' names cleaning.\n",
    "cdd_binance_df[\"currency_pair\"] = cdd_binance_df[\"currency_pair\"].str.replace(\n",
    "    \"/\", \"_\"\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c01e0a5f",
   "metadata": {
    "ExecuteTime": {
     "start_time": "2022-01-14T10:07:01.025Z"
